theorem tangent_map_comp (hg : Mdifferentiable I' I'' g) (hf : Mdifferentiable I I' f) :
    tangentMap I I'' (g ‚àò f) = tangentMap I' I'' g ‚àò tangentMap I I' f := by
  ext p : 1
