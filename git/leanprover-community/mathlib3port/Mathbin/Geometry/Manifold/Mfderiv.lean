  have mem : I x ‚àà I.symm ‚Åª¬π' (e.symm ‚â´‚Çï chart_at H (e.symm x)).Source ‚à© range I := by
    simp' only [‚Üê hx] with mfld_simps
  have : e.symm.trans (chart_at H (e.symm x)) ‚àà contDiffGroupoid ‚àû I := HasGroupoid.compatible _ h (chart_mem_atlas H _)
  have A :
    ContDiffOn ùïú ‚àû (I ‚àò e.symm.trans (chart_at H (e.symm x)) ‚àò I.symm)
      (I.symm ‚Åª¬π' (e.symm.trans (chart_at H (e.symm x))).Source ‚à© range I) :=
    this.1
  have B := A.differentiable_on le_top (I x) mem
  simp' only with mfld_simps  at B
  rw [inter_comm, differentiable_within_at_inter] at B
  ¬∑ simpa only with mfld_simps
    
  ¬∑ apply IsOpen.mem_nhds ((LocalHomeomorph.open_source _).Preimage I.continuous_symm) mem.1
    

theorem mdifferentiable_on_atlas_symm (h : e ‚àà Atlas H M) : MdifferentiableOn I I e.symm e.Target := fun x hx =>
  (mdifferentiable_at_atlas_symm I h hx).MdifferentiableWithinAt

theorem mdifferentiable_of_mem_atlas (h : e ‚àà Atlas H M) : e.Mdifferentiable I I :=
  ‚ü®mdifferentiable_on_atlas I h, mdifferentiable_on_atlas_symm I h‚ü©

theorem mdifferentiable_chart (x : M) : (chartAt H x).Mdifferentiable I I :=
  mdifferentiable_of_mem_atlas _ (chart_mem_atlas _ _)

/-- The derivative of the chart at a base point is the chart of the tangent bundle, composed with
the identification between the tangent bundle of the model space and the product space. -/
theorem tangent_map_chart {p q : TangentBundle I M} (h : q.1 ‚àà (chartAt H p.1).Source) :
    tangentMap I I (chartAt H p.1) q =
      (Equiv‚Çì.sigmaEquivProd _ _).symm ((chartAt (ModelProd H E) p : TangentBundle I M ‚Üí ModelProd H E) q) :=
  by
  dsimp' [‚Üê tangentMap]
  rw [MdifferentiableAt.mfderiv]
  ¬∑ rfl
    
  ¬∑ exact mdifferentiable_at_atlas _ (chart_mem_atlas _ _) h
    

/-- The derivative of the inverse of the chart at a base point is the inverse of the chart of the
tangent bundle, composed with the identification between the tangent bundle of the model space and
the product space. -/
theorem tangent_map_chart_symm {p : TangentBundle I M} {q : TangentBundle I H} (h : q.1 ‚àà (chartAt H p.1).Target) :
    tangentMap I I (chartAt H p.1).symm q =
      ((chartAt (ModelProd H E) p).symm : ModelProd H E ‚Üí TangentBundle I M) ((Equiv‚Çì.sigmaEquivProd H E) q) :=
  by
  dsimp' only [‚Üê tangentMap]
  rw [MdifferentiableAt.mfderiv (mdifferentiable_at_atlas_symm _ (chart_mem_atlas _ _) h)]
  -- a trivial instance is needed after the rewrite, handle it right now.
  rotate_left
  ¬∑ infer_instance
    
  simp' only [‚Üê ContinuousLinearMap.coe_coe, ‚Üê BasicSmoothVectorBundleCore.chart, ‚Üê h, ‚Üê tangentBundleCore, ‚Üê
    BasicSmoothVectorBundleCore.toTopologicalVectorBundleCore, ‚Üê chart_at, ‚Üê Sigma.mk.inj_iff] with mfld_simps

end Charts

end SpecificFunctions

/-! ### Differentiable local homeomorphisms -/


namespace LocalHomeomorph.Mdifferentiable

variable {ùïú : Type _} [NontriviallyNormedField ùïú] {E : Type _} [NormedAddCommGroup E] [NormedSpace ùïú E] {H : Type _}
  [TopologicalSpace H] {I : ModelWithCorners ùïú E H} {M : Type _} [TopologicalSpace M] [ChartedSpace H M] {E' : Type _}
  [NormedAddCommGroup E'] [NormedSpace ùïú E'] {H' : Type _} [TopologicalSpace H'] {I' : ModelWithCorners ùïú E' H'}
  {M' : Type _} [TopologicalSpace M'] [ChartedSpace H' M'] {E'' : Type _} [NormedAddCommGroup E''] [NormedSpace ùïú E'']
  {H'' : Type _} [TopologicalSpace H''] {I'' : ModelWithCorners ùïú E'' H''} {M'' : Type _} [TopologicalSpace M'']
  [ChartedSpace H'' M''] {e : LocalHomeomorph M M'} (he : e.Mdifferentiable I I') {e' : LocalHomeomorph M' M''}

include he

theorem symm : e.symm.Mdifferentiable I' I :=
  ‚ü®he.2, he.1‚ü©

protected theorem mdifferentiable_at {x : M} (hx : x ‚àà e.Source) : MdifferentiableAt I I' e x :=
  (he.1 x hx).MdifferentiableAt (IsOpen.mem_nhds e.open_source hx)

theorem mdifferentiable_at_symm {x : M'} (hx : x ‚àà e.Target) : MdifferentiableAt I' I e.symm x :=
  (he.2 x hx).MdifferentiableAt (IsOpen.mem_nhds e.open_target hx)

variable [SmoothManifoldWithCorners I M] [SmoothManifoldWithCorners I' M'] [SmoothManifoldWithCorners I'' M'']

theorem symm_comp_deriv {x : M} (hx : x ‚àà e.Source) :
    (mfderiv I' I e.symm (e x)).comp (mfderiv I I' e x) = ContinuousLinearMap.id ùïú (TangentSpace I x) := by
  have : mfderiv I I (e.symm ‚àò e) x = (mfderiv I' I e.symm (e x)).comp (mfderiv I I' e x) :=
    mfderiv_comp x (he.mdifferentiable_at_symm (e.map_source hx)) (he.mdifferentiable_at hx)
  rw [‚Üê this]
  have : mfderiv I I (_root_.id : M ‚Üí M) x = ContinuousLinearMap.id _ _ := mfderiv_id I
  rw [‚Üê this]
  apply Filter.EventuallyEq.mfderiv_eq
  have : e.source ‚àà ùìù x := IsOpen.mem_nhds e.open_source hx
  exact
    Filter.mem_of_superset this
      (by
        mfld_set_tac)

theorem comp_symm_deriv {x : M'} (hx : x ‚àà e.Target) :
    (mfderiv I I' e (e.symm x)).comp (mfderiv I' I e.symm x) = ContinuousLinearMap.id ùïú (TangentSpace I' x) :=
  he.symm.symm_comp_deriv hx

/-- The derivative of a differentiable local homeomorphism, as a continuous linear equivalence
between the tangent spaces at `x` and `e x`. -/
protected def mfderiv {x : M} (hx : x ‚àà e.Source) : TangentSpace I x ‚âÉL[ùïú] TangentSpace I' (e x) :=
  { mfderiv I I' e x with invFun := mfderiv I' I e.symm (e x), continuous_to_fun := (mfderiv I I' e x).cont,
    continuous_inv_fun := (mfderiv I' I e.symm (e x)).cont,
    left_inv := fun y => by
      have : (ContinuousLinearMap.id _ _ : TangentSpace I x ‚ÜíL[ùïú] TangentSpace I x) y = y := rfl
      conv_rhs => rw [‚Üê this, ‚Üê he.symm_comp_deriv hx]
      rfl,
    right_inv := fun y => by
      have : (ContinuousLinearMap.id ùïú _ : TangentSpace I' (e x) ‚ÜíL[ùïú] TangentSpace I' (e x)) y = y := rfl
      conv_rhs => rw [‚Üê this, ‚Üê he.comp_symm_deriv (e.map_source hx)]
      rw [e.left_inv hx]
      rfl }

theorem mfderiv_bijective {x : M} (hx : x ‚àà e.Source) : Function.Bijective (mfderiv I I' e x) :=
  (he.mfderiv hx).Bijective

theorem mfderiv_injective {x : M} (hx : x ‚àà e.Source) : Function.Injective (mfderiv I I' e x) :=
  (he.mfderiv hx).Injective

theorem mfderiv_surjective {x : M} (hx : x ‚àà e.Source) : Function.Surjective (mfderiv I I' e x) :=
  (he.mfderiv hx).Surjective

theorem ker_mfderiv_eq_bot {x : M} (hx : x ‚àà e.Source) : (mfderiv I I' e x).ker = ‚ä• :=
  (he.mfderiv hx).toLinearEquiv.ker

theorem range_mfderiv_eq_top {x : M} (hx : x ‚àà e.Source) : (mfderiv I I' e x).range = ‚ä§ :=
  (he.mfderiv hx).toLinearEquiv.range

theorem range_mfderiv_eq_univ {x : M} (hx : x ‚àà e.Source) : Range (mfderiv I I' e x) = univ :=
  (he.mfderiv_surjective hx).range_eq

theorem trans (he' : e'.Mdifferentiable I' I'') : (e.trans e').Mdifferentiable I I'' := by
  constructor
  ¬∑ intro x hx
    simp' only with mfld_simps  at hx
    exact ((he'.mdifferentiable_at hx.2).comp _ (he.mdifferentiable_at hx.1)).MdifferentiableWithinAt
    
  ¬∑ intro x hx
    simp' only with mfld_simps  at hx
    exact ((he.symm.mdifferentiable_at hx.2).comp _ (he'.symm.mdifferentiable_at hx.1)).MdifferentiableWithinAt
    

end LocalHomeomorph.Mdifferentiable

/-! ### Differentiability of `ext_chart_at` -/


section extChartAt

variable {ùïú : Type _} [NontriviallyNormedField ùïú] {E : Type _} [NormedAddCommGroup E] [NormedSpace ùïú E] {H : Type _}
  [TopologicalSpace H] (I : ModelWithCorners ùïú E H) {M : Type _} [TopologicalSpace M] [ChartedSpace H M]
  [SmoothManifoldWithCorners I M] {s : Set M} {x y : M}

theorem has_mfderiv_at_ext_chart_at (h : y ‚àà (chartAt H x).Source) :
    HasMfderivAt I ùìò(ùïú, E) (extChartAt I x) y (mfderiv I I (chartAt H x) y : _) :=
  I.HasMfderivAt.comp y ((mdifferentiable_chart I x).MdifferentiableAt h).HasMfderivAt

theorem has_mfderiv_within_at_ext_chart_at (h : y ‚àà (chartAt H x).Source) :
    HasMfderivWithinAt I ùìò(ùïú, E) (extChartAt I x) s y (mfderiv I I (chartAt H x) y : _) :=
  (has_mfderiv_at_ext_chart_at I h).HasMfderivWithinAt

theorem mdifferentiable_at_ext_chart_at (h : y ‚àà (chartAt H x).Source) :
    MdifferentiableAt I ùìò(ùïú, E) (extChartAt I x) y :=
  (has_mfderiv_at_ext_chart_at I h).MdifferentiableAt

theorem mdifferentiable_on_ext_chart_at : MdifferentiableOn I ùìò(ùïú, E) (extChartAt I x) (chartAt H x).Source :=
  fun y hy => (has_mfderiv_within_at_ext_chart_at I hy).MdifferentiableWithinAt

end extChartAt

/-! ### Unique derivative sets in manifolds -/


section UniqueMdiff

variable {ùïú : Type _} [NontriviallyNormedField ùïú] {E : Type _} [NormedAddCommGroup E] [NormedSpace ùïú E] {H : Type _}
  [TopologicalSpace H] {I : ModelWithCorners ùïú E H} {M : Type _} [TopologicalSpace M] [ChartedSpace H M]
  [SmoothManifoldWithCorners I M] {E' : Type _} [NormedAddCommGroup E'] [NormedSpace ùïú E'] {H' : Type _}
  [TopologicalSpace H'] {I' : ModelWithCorners ùïú E' H'} {M' : Type _} [TopologicalSpace M'] [ChartedSpace H' M']
  {s : Set M}

/-- If a set has the unique differential property, then its image under a local
diffeomorphism also has the unique differential property. -/
theorem UniqueMdiffOn.unique_mdiff_on_preimage [SmoothManifoldWithCorners I' M'] (hs : UniqueMdiffOn I s)
    {e : LocalHomeomorph M M'} (he : e.Mdifferentiable I I') : UniqueMdiffOn I' (e.Target ‚à© e.symm ‚Åª¬π' s) := by
  /- Start from a point `x` in the image, and let `z` be its preimage. Then the unique
    derivative property at `x` is expressed through `ext_chart_at I' x`, and the unique
    derivative property at `z` is expressed through `ext_chart_at I z`. We will argue that
    the composition of these two charts with `e` is a local diffeomorphism in vector spaces,
    and therefore preserves the unique differential property thanks to lemma
    `has_fderiv_within_at.unique_diff_within_at`, saying that a differentiable function with onto
    derivative preserves the unique derivative property.-/
  intro x hx
  let z := e.symm x
  have z_source : z ‚àà e.source := by
    simp' only [‚Üê hx.1] with mfld_simps
  have zx : e z = x := by
    simp' only [‚Üê z, ‚Üê hx.1] with mfld_simps
  let F := extChartAt I z
  -- the unique derivative property at `z` is expressed through its preferred chart,
  -- that we call `F`.
  have B : UniqueDiffWithinAt ùïú (F.symm ‚Åª¬π' (s ‚à© (e.source ‚à© e ‚Åª¬π' (extChartAt I' x).Source)) ‚à© F.target) (F z) := by
    have : UniqueMdiffWithinAt I s z := hs _ hx.2
    have S : e.source ‚à© e ‚Åª¬π' (extChartAt I' x).Source ‚àà ùìù z := by
      apply IsOpen.mem_nhds
      apply e.continuous_on.preimage_open_of_open e.open_source (ext_chart_at_open_source I' x)
      simp' only [‚Üê z_source, ‚Üê zx] with mfld_simps
    have := this.inter S
    rw [unique_mdiff_within_at_iff] at this
    exact this
  -- denote by `G` the change of coordinate, i.e., the composition of the two extended charts and
  -- of `e`
  let G := F.symm ‚â´ e.to_local_equiv ‚â´ extChartAt I' x
  -- `G` is differentiable
  have Diff : ((chart_at H z).symm ‚â´‚Çï e ‚â´‚Çï chart_at H' x).Mdifferentiable I I' := by
    have A := mdifferentiable_of_mem_atlas I (chart_mem_atlas H z)
    have B := mdifferentiable_of_mem_atlas I' (chart_mem_atlas H' x)
    exact A.symm.trans (he.trans B)
  have Mmem : (chart_at H z : M ‚Üí H) z ‚àà ((chart_at H z).symm ‚â´‚Çï e ‚â´‚Çï chart_at H' x).Source := by
    simp' only [‚Üê z_source, ‚Üê zx] with mfld_simps
  have A : DifferentiableWithinAt ùïú G (range I) (F z) := by
    refine' (Diff.mdifferentiable_at Mmem).2.congr (fun p hp => _) _ <;> simp' only [‚Üê G, ‚Üê F] with mfld_simps
  -- let `G'` be its derivative
  let G' := fderivWithin ùïú G (range I) (F z)
  have D‚ÇÅ : HasFderivWithinAt G G' (range I) (F z) := A.has_fderiv_within_at
  have D‚ÇÇ : HasFderivWithinAt G G' (F.symm ‚Åª¬π' (s ‚à© (e.source ‚à© e ‚Åª¬π' (extChartAt I' x).Source)) ‚à© F.target) (F z) :=
    D‚ÇÅ.mono
      (by
        mfld_set_tac)
  -- The derivative `G'` is onto, as it is the derivative of a local diffeomorphism, the composition
  -- of the two charts and of `e`.
  have C : DenseRange (G' : E ‚Üí E') := by
    have : G' = mfderiv I I' ((chart_at H z).symm ‚â´‚Çï e ‚â´‚Çï chart_at H' x) ((chart_at H z : M ‚Üí H) z) := by
      rw [(Diff.mdifferentiable_at Mmem).mfderiv]
      rfl
    rw [this]
    exact (Diff.mfderiv_surjective Mmem).DenseRange
  -- key step: thanks to what we have proved about it, `G` preserves the unique derivative property
  have key :
    UniqueDiffWithinAt ùïú (G '' (F.symm ‚Åª¬π' (s ‚à© (e.source ‚à© e ‚Åª¬π' (extChartAt I' x).Source)) ‚à© F.target)) (G (F z)) :=
    D‚ÇÇ.unique_diff_within_at B C
  have : G (F z) = (extChartAt I' x) x := by
    dsimp' [‚Üê G, ‚Üê F]
    simp' only [‚Üê hx.1] with mfld_simps
  rw [this] at key
  apply key.mono
  show
    G '' (F.symm ‚Åª¬π' (s ‚à© (e.source ‚à© e ‚Åª¬π' (extChartAt I' x).Source)) ‚à© F.target) ‚äÜ
      (extChartAt I' x).symm ‚Åª¬π' e.target ‚à© (extChartAt I' x).symm ‚Åª¬π' (e.symm ‚Åª¬π' s) ‚à© range I'
  rw [image_subset_iff]
  mfld_set_tac

/-- If a set in a manifold has the unique derivative property, then its pullback by any extended
chart, in the vector space, also has the unique derivative property. -/
theorem UniqueMdiffOn.unique_diff_on_target_inter (hs : UniqueMdiffOn I s) (x : M) :
    UniqueDiffOn ùïú ((extChartAt I x).Target ‚à© (extChartAt I x).symm ‚Åª¬π' s) := by
  -- this is just a reformulation of `unique_mdiff_on.unique_mdiff_on_preimage`, using as `e`
  -- the local chart at `x`.
  intro z hz
  simp' only with mfld_simps  at hz
  have : (chart_at H x).Mdifferentiable I I := mdifferentiable_chart _ _
  have T := (hs.unique_mdiff_on_preimage this) (I.symm z)
  simp' only [‚Üê hz.left.left, ‚Üê hz.left.right, ‚Üê hz.right, ‚Üê UniqueMdiffWithinAt] with mfld_simps  at T‚ä¢
  convert T using 1
  rw [@preimage_comp _ _ _ _ (chart_at H x).symm]
  mfld_set_tac

/-- When considering functions between manifolds, this statement shows up often. It entails
the unique differential of the pullback in extended charts of the set where the function can
be read in the charts. -/
theorem UniqueMdiffOn.unique_diff_on_inter_preimage (hs : UniqueMdiffOn I s) (x : M) (y : M') {f : M ‚Üí M'}
    (hf : ContinuousOn f s) :
    UniqueDiffOn ùïú ((extChartAt I x).Target ‚à© (extChartAt I x).symm ‚Åª¬π' (s ‚à© f ‚Åª¬π' (extChartAt I' y).Source)) := by
  have : UniqueMdiffOn I (s ‚à© f ‚Åª¬π' (extChartAt I' y).Source) := by
    intro z hz
    apply (hs z hz.1).inter'
    apply (hf z hz.1).preimage_mem_nhds_within
    exact IsOpen.mem_nhds (ext_chart_at_open_source I' y) hz.2
  exact this.unique_diff_on_target_inter _

variable {F : Type _} [NormedAddCommGroup F] [NormedSpace ùïú F] (Z : BasicSmoothVectorBundleCore I M F)

/-- In a smooth fiber bundle constructed from core, the preimage under the projection of a set with
unique differential in the basis also has unique differential. -/
theorem UniqueMdiffOn.smooth_bundle_preimage (hs : UniqueMdiffOn I s) :
    UniqueMdiffOn (I.Prod ùìò(ùïú, F)) (Z.toTopologicalVectorBundleCore.proj ‚Åª¬π' s) := by
  /- Using a chart (and the fact that unique differentiability is invariant under charts), we
    reduce the situation to the model space, where we can use the fact that products respect
    unique differentiability. -/
  intro p hp
  replace hp : p.fst ‚àà s
  ¬∑ simpa only with mfld_simps using hp
    
  let e‚ÇÄ := chart_at H p.1
  let e := chart_at (ModelProd H F) p
  -- It suffices to prove unique differentiability in a chart
  suffices h : UniqueMdiffOn (I.prod ùìò(ùïú, F)) (e.target ‚à© e.symm ‚Åª¬π' (Z.to_topological_vector_bundle_core.proj ‚Åª¬π' s))
  ¬∑ have A :
      UniqueMdiffOn (I.prod ùìò(ùïú, F))
        (e.symm.target ‚à© e.symm.symm ‚Åª¬π' (e.target ‚à© e.symm ‚Åª¬π' (Z.to_topological_vector_bundle_core.proj ‚Åª¬π' s))) :=
      by
      apply h.unique_mdiff_on_preimage
      exact (mdifferentiable_of_mem_atlas _ (chart_mem_atlas _ _)).symm
      infer_instance
    have :
      p ‚àà e.symm.target ‚à© e.symm.symm ‚Åª¬π' (e.target ‚à© e.symm ‚Åª¬π' (Z.to_topological_vector_bundle_core.proj ‚Åª¬π' s)) := by
      simp' only [‚Üê e, ‚Üê hp] with mfld_simps
    apply (A _ this).mono
    intro q hq
    simp' only [‚Üê e, ‚Üê LocalHomeomorph.left_inv _ hq.1] with mfld_simps  at hq
    simp' only [‚Üê hq] with mfld_simps
    
  -- rewrite the relevant set in the chart as a direct product
  have :
    (fun p : E √ó F => (I.symm p.1, p.snd)) ‚Åª¬π' e.target ‚à©
          (fun p : E √ó F => (I.symm p.1, p.snd)) ‚Åª¬π' (e.symm ‚Åª¬π' (Sigma.fst ‚Åª¬π' s)) ‚à©
        range I √óÀ¢ univ =
      (I.symm ‚Åª¬π' (e‚ÇÄ.target ‚à© e‚ÇÄ.symm ‚Åª¬π' s) ‚à© range I) √óÀ¢ univ :=
    by
    mfld_set_tac
  intro q hq
  replace hq : q.1 ‚àà (chart_at H p.1).Target ‚àß ((chart_at H p.1).symm : H ‚Üí M) q.1 ‚àà s
  ¬∑ simpa only with mfld_simps using hq
    
  simp' only [‚Üê UniqueMdiffWithinAt, ‚Üê ModelWithCorners.prod, ‚Üê preimage_inter, ‚Üê this] with mfld_simps
  -- apply unique differentiability of products to conclude
  apply UniqueDiffOn.prod _ unique_diff_on_univ
  ¬∑ simp' only [‚Üê hq] with mfld_simps
    
  ¬∑ intro x hx
    have A : UniqueMdiffOn I (e‚ÇÄ.target ‚à© e‚ÇÄ.symm ‚Åª¬π' s) := by
      apply hs.unique_mdiff_on_preimage
      exact mdifferentiable_of_mem_atlas _ (chart_mem_atlas _ _)
      infer_instance
    simp' only [‚Üê UniqueMdiffOn, ‚Üê UniqueMdiffWithinAt, ‚Üê preimage_inter] with mfld_simps  at A
    have B := A (I.symm x) hx.1.1 hx.1.2
    rwa [‚Üê preimage_inter, ModelWithCorners.right_inv _ hx.2] at B
    

theorem UniqueMdiffOn.tangent_bundle_proj_preimage (hs : UniqueMdiffOn I s) :
    UniqueMdiffOn I.tangent (TangentBundle.proj I M ‚Åª¬π' s) :=
  hs.smooth_bundle_preimage _

end UniqueMdiff

